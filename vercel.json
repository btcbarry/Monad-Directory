{
  "version": 2,
  "builds": [
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "*.js", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/submit", "dest": "/submit.html" },
    { "src": "/projects.json", "dest": "/public/projects.json" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
