{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/projects.json",
      "dest": "/public/projects.json"
    },
    {
      "src": "/submit",
      "dest": "/submit.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
