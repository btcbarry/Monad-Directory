{
  "version": 2,
  "routes": [
    { 
      "src": "/public/projects.json",
      "dest": "/public/projects.json",
      "headers": {
        "cache-control": "public, max-age=0, must-revalidate"
      }
    },
    { 
      "src": "/submit",
      "dest": "/submit.html"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1",
      "headers": {
        "cache-control": "public, max-age=0, must-revalidate"
      }
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
